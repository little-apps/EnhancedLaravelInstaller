    "require": {
        "php": ">=7.1.3",
        "rachidlaasri/laravel-installer": "^4.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Nick H",
            "email": "nick@little-apps.com"
        }
    ],
	"autoload": {
        "psr-4": {
            "LittleApps\\EnhancedLaravelInstaller\\": "src/"
        }
    },
	"extra": {
        "laravel": {
            "providers": [
                "LittleApps\\EnhancedLaravelInstaller\\Providers\\LaravelInstallerServiceProvider"
            ]
        }
	}
}
